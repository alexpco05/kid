import java.util.ArrayList;
import java.util.Arrays;
public class array {
    public static void main(String[] args){
        int X = 0;
        ArrayList<Integer> myArray = new ArrayList<Integer>
        (Arrays.asList(1,2,3,4,5));
for ( int i = 0 ; i <= 4 ; i ++ )
{X = X + myArray.get(i);
}
System.out.println(X);
}
}
